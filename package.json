{
  "name": "@clubmed/root",
  "type": "module",
  "scripts": {
    "build": "monorepo build --verbose",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "typescript": "tsc --init",
    "test": "vitest run",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.vue",
    "lint:fix": "yarn lint --fix",
    "prettier:fix": "prettier . --write",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:test": "yarn test-storybook",
    "storybook:test:ci": "yarn test-storybook --maxWorkers=2",
    "build-storybook": "yarn storybook:build",
    "prepare": "is-ci && husky install",
    "chromatic": "npx chromatic",
    "release": "semantic-release",
    "release:dryRun": "semantic-release --dry-run"
  },
  "dependencies": {
    "@vueuse/core": "10.11.0",
    "axios": "1.7.7",
    "lucide-vue-next": "^0.436.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.6.1",
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@semantic-release/commit-analyzer": "13.0.0",
    "@semantic-release/github": "11.0.0",
    "@semantic-release/npm": "12.0.1",
    "@semantic-release/release-notes-generator": "14.0.1",
    "@storybook/addon-a11y": "^8.2.2",
    "@storybook/addon-essentials": "^8.2.2",
    "@storybook/addon-interactions": "^8.2.2",
    "@storybook/addon-links": "^8.2.2",
    "@storybook/addon-storysource": "^8.2.2",
    "@storybook/addon-themes": "^8.2.2",
    "@storybook/blocks": "^8.2.2",
    "@storybook/test": "^8.2.2",
    "@storybook/test-runner": "0.19.0",
    "@storybook/vue3": "^8.2.2",
    "@storybook/vue3-vite": "^8.2.2",
    "@testing-library/jest-dom": "6.4.6",
    "@testing-library/vue": "8.1.0",
    "@tsed/monorepo-utils": "2.3.6",
    "@typescript-eslint/eslint-plugin": "7.16.0",
    "@typescript-eslint/parser": "7.16.0",
    "@vitejs/plugin-vue": "5.0.5",
    "@vitest/coverage-v8": "^1.6.0",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/eslint-config-typescript": "13.0.0",
    "autoprefixer": "^10.4.19",
    "chromatic": "^11.5.5",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-tailwindcss": "3.17.4",
    "eslint-plugin-vitest": "0.5.4",
    "eslint-plugin-vue": "9.27.0",
    "husky": "9.0.11",
    "install": "^0.13.0",
    "is-ci": "^3.0.1",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.2.7",
    "playwright": "1.45.1",
    "postcss": "^8.4.39",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.5.14",
    "semantic-release": "24.1.3",
    "semantic-release-slack-bot": "4.0.2",
    "storybook": "^8.2.2",
    "storybook-addon-manual-mocks": "^1.0.4",
    "tailwindcss": "3.4.4",
    "ts-node": "10.9.2",
    "typescript": "^5.3.3",
    "vite": "5.3.3",
    "vitepress": "1.3.1",
    "vitest": "1.6.0",
    "vue": "3.4.31"
  },
  "lint-staged": {
    "docs/.vitepress/theme/**/*.{ts,tsx,js,jsx,vue}": [
      "eslint --fix"
    ],
    "docs/**/*.{ts,tsx,js,jsx,vue}": [
      "eslint --fix"
    ],
    "*.{ts,tsx,js,jsx,vue}": [
      "eslint --fix"
    ],
    "**/*.{css,json,md,yml,yaml}": "prettier --write"
  },
  "resolutions": {
    "vite": "5.3.3"
  },
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "yarn@4.1.1",
  "version": ""
}

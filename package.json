{
  "name": "@clubmed/root",
  "type": "module",
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "typescript": "tsc --init",
    "test": "vitest run",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.vue",
